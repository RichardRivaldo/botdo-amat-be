{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "rules": {
        "no-console": "off",
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018
    }
}
